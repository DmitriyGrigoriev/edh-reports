  </DataSources>
  <DataSets>
    <DataSet Name="LocalFTS">
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RepName">
            <Value>=Parameters!RepName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Inn">
            <Value>=Parameters!Inn.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SD">
            <Value>=Parameters!SD.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ED">
            <Value>=Parameters!ED.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>INSERT INTO aLog (aDate, aLogin, aReport)
VALUES   (GetDate(), SYSTEM_USER,@RepName); 
Select G07X, CASE When G021='7725216105' Then G022 Else G082 End as FName, GD1,
       G011+' '+(Select Left(Max(G37),2)
                 From tDclTovar
                 Where G07X=tDCLHead.G07X) as rezh,
       (Select Sum(G45)
        From tDclTovar
        Where G07X=tDCLHead.G07X) as TamSto,
       (Select Sum(G474)
        From tDCLPLATR
        Where G471 like '1%'
          and G07X=tDCLHead.G07X) as TamSbor,
       (Select Sum(G474)
        From tDCLPLATR
        Where G471 like '2%'
          and  G07X=tDCLHead.G07X) as TamPoshl,
       (Select Sum(G474)
        From tDCLPLATR
        Where G471 like '5%'
          and G07X=tDCLHead.G07X) as NDS,
       (Select SubString(MName, 4, 40)
        From aMonthQurtal
        Where Id=Month(GD1))+' '+Convert(VarChar(4), Year(GD1)) As MonthYearName,
       Month(GD1)+Year(GD1)*12 As MonthYear
From tDCLHead
Where (G081=@Inn or G021=@Inn) --7725216105
  and GD1&gt;=@SD
  and GD1&lt;=@ED
  and not exists(Select G33
                 From tDclTovar
                 Where G07X=tDCLHead.G07X
                   and G33 like '30%')
Order By MonthYear, G07X</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
