  </DataSources>
  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>LacalFTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@inn">
            <Value>=Parameters!inn.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RepName">
            <Value>=Parameters!RepName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SD">
            <Value>=Parameters!SD.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ED">
            <Value>=Parameters!ED.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>INSERT INTO aLog (aDate, aLogin, aReport)
VALUES   (GetDate(), SYSTEM_USER,@RepName); 
Select tDclHead.G07X, tDclHead.GD1, Sum(K477) As Kts,
       case max(G081) when @inn then max(G082) else max(G022) end as firm
From OLAP.LocalFTS.dbo.tDclHead join
     OLAP.LocalFTS.dbo.[tKTCPLTIZ] on tDclHead.G07X=[tKTCPLTIZ].G07X
where (g021 like @inn
   or g081 like @inn)
  and GD1&gt;=@SD
  and GD1&lt;=@ED
Group By tDclHead.G07X, tDclHead.GD1
Order by 2,1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="G07X">
          <DataField>G07X</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GD1">
          <DataField>GD1</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Kts">
          <DataField>Kts</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="firm">
          <DataField>firm</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
