      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     SLEG.Description[Номер ДТ], SIH.[Shortcut Dimension 1 Code] [ЦФО], SIH.[Shortcut Dimension 2 Code] [Код группы], 
                      Cust.[Name] + Cust.[Name 2] [Импортер/Экспортер], SIH.[Sell-to Customer Name] + SIH.[Sell-to Customer Name 2] [Плательщик], 
                      SIH.[No_] [Номер счета], SIH.[Posting Date] [Дата счета], SIL.[Line No] [Номер путнкта счета], LEFT(SLE.[Service Description], 30) [Услуга], 
                      SIL.[Quantity] [Количество], SIL.[Unit of Measure] [Ед. Изм.], SIL.[Unit Price] [Цена], SIL.[Line Amount] [Сумма], 
                      SIL2.[Invoice Amount] [Общая сумма счета без НДС], SIH.[Currency Code] [Код валюты], D_OPL.[Дата оплаты]
FROM         dbo.[NAVSales Invoice Header] SIH JOIN
                          (SELECT     [Document No_], ROW_NUMBER() OVER (PARTITION BY [Document No_]
                            ORDER BY [Line No_]) [Line No], [Services Ledger Entry No_], [No_], [Quantity], [Unit of Measure], [Unit Price], [Line Amount]
FROM         dbo.[NAVSales Invoice Line]
WHERE     Type = 3) SIL ON SIH.[No_] = SIL.[Document No_] JOIN
    (SELECT     [Document No_], sum([Line Amount]) [Invoice Amount]
      FROM          dbo.[NAVSales Invoice Line]
      WHERE      Type = 3
      GROUP BY [Document No_]) SIL2 ON SIH.[No_] = SIL2.[Document No_] JOIN
dbo.[NAVService Ledger Entry GTD] SLEG ON SIL.[Services Ledger Entry No_] = SLEG.[Entry No_] LEFT JOIN
dbo.[NAVServices Ledger Entry] SLE ON SIL.[Services Ledger Entry No_] = SLE.[Entry No_] LEFT JOIN
dbo.[NAVCustomer] Cust ON SLE.[Customer No_] = Cust.[No_] LEFT JOIN
    (SELECT     [Document No_], max([Due Date]) [Дата оплаты]
      FROM          (SELECT     [Entry No_], [Document No_], [Due Date]
                              FROM          (SELECT     CLE1.[Entry No_], CLE1.[Document No_], CLE2.[Posting Date] [Due Date]
                                                      FROM          dbo.[NAVCust_ Ledger Entry] CLE1 JOIN
                                                                             dbo.[NAVCust_ Ledger Entry] CLE2 ON CLE1.[Closed by Entry No_] = CLE2.[Entry No_]
                                                      WHERE      CLE1.[Document Type] = 2) CLE_1
                              UNION
                              SELECT     [Entry No_], [Document No_], [Due Date]
                              FROM         (SELECT     CLE3.[Entry No_], CLE3.[Document No_], CLE4.[Posting Date] [Due Date]
                                                     FROM          dbo.[NAVCust_ Ledger Entry] CLE3 JOIN
                                                                            dbo.[NAVCust_ Ledger Entry] CLE4 ON CLE4.[Closed by Entry No_] = CLE3.[Entry No_]
                                                     WHERE      CLE3.[Document Type] = 2) CLE_2) CLE_SUM JOIN
                                 (SELECT     [Cust_ Ledger Entry No_]
                                   FROM          dbo.[NAVDetailed Cust_ Ledg_ Entry]
                                   GROUP BY [Cust_ Ledger Entry No_]
                                   HAVING      SUM(Amount) = 0) OST0 ON CLE_SUM.[Entry No_] = OST0.[Cust_ Ledger Entry No_]
      GROUP BY [Document No_]) D_OPL ON SIH.[No_] = D_OPL.[Document No_]
WHERE     SIH.[Posting Date] &gt; getdate() - 92 AND SIH.[Shortcut Dimension 1 Code] = @CFO AND SLEG.[Entry Type] = 1 AND len(SLEG.Description) &gt; 21 AND 
                      SIH.[No_] NOT IN
                          (SELECT     CLE_.[Document No_]
                            FROM          dbo.[NAVCust_ Ledger Entry] CLE_ JOIN
                                                   dbo.[NAVCust_ Ledger Entry] CLE2_ ON CLE_.[Closed by Entry No_] = CLE2_.[Entry No_]
                            WHERE      CLE_.[Document Type] = 2 AND CLE2_.[Document Type] = 3)
UNION
SELECT     SLEG.Description[Номер ДТ], SH.[Shortcut Dimension 1 Code] [ЦФО], SH.[Shortcut Dimension 2 Code] [Код группы], 
                      Cust.[Name] + Cust.[Name 2] [Импортер/Экспортер], SH.[Sell-to Customer Name] + SH.[Sell-to Customer Name 2] [Плательщик], 
                      SH.[Posting No_] [Номер счета], SH.[Posting Date], SL.[Line No] [Номер путнкта счета], LEFT(SLE.[Service Description], 30) [Услуга], 
                      SL.[Quantity] [Количество], SL.[Unit of Measure] [Ед. Изм.], SL.[Unit Price] [Цена], SL.[Line Amount] [Сумма], 
                      SL2.[Invoice Amount] [Общая сумма счета без НДС], SH.[Currency Code] [Код валюты], '' [Дата оплаты]
FROM         dbo.[NAVSales Header] SH JOIN
                          (SELECT     [Document No_], ROW_NUMBER() OVER (PARTITION BY [Document No_]
                            ORDER BY [Line No_]) [Line No], [Services Ledger Entry No_], [No_], [Quantity], [Unit of Measure], [Unit Price], [Line Amount]
FROM         dbo.[NAVSales Line]
WHERE     Type = 3) SL ON SH.[No_] = SL.[Document No_] JOIN
    (SELECT     [Document No_], sum([Line Amount]) [Invoice Amount]
      FROM          dbo.[NAVSales Line]
      WHERE      Type = 3
      GROUP BY [Document No_]) SL2 ON SH.[No_] = SL2.[Document No_] JOIN
dbo.[NAVService Ledger Entry GTD] SLEG ON SL.[Services Ledger Entry No_] = SLEG.[Entry No_] JOIN
dbo.[NAVServices Ledger Entry] SLE ON SL.[Services Ledger Entry No_] = SLE.[Entry No_] JOIN
dbo.[NAVCustomer] Cust ON SLE.[Customer No_] = Cust.[No_]
WHERE     SH.[Posting Date] &gt; getdate() - 92 AND SH.[Shortcut Dimension 1 Code] = @CFO AND SH.[Current Stage] = 'ПЕРЕДАН' AND len(SH.[Posting No_]) 
                      &gt; 9 AND SLEG.[Entry Type] = 1 AND len(SLEG.Description) &gt; 21</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CFO">
            <Value>=Parameters!CFO.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="CFO">
      <Fields>
        <Field Name="Global_Dimension_1_Code">
          <DataField>Global Dimension 1 Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     [Global Dimension 1 Code]
FROM         [NAVCust_ Ledger Entry]
GROUP BY [Global Dimension 1 Code]
HAVING      ([Global Dimension 1 Code] &lt;&gt; '')
ORDER BY [Global Dimension 1 Code]</CommandText>
      </Query>
    </DataSet>
