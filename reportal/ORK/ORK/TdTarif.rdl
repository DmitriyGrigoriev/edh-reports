          <DataField>№ счета</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT 
GLE.[GTDNo_] [№ ДТ],
GLE.[Posting Date] [Дата выпуска],
Cust.[Name] + Cust.[Name 2] [Импортер/Экспортер],
GLE.[Responsibility Center] [Площадка],
SLE.[Posting Date and Time] [Дата и время тарификации],
CASE 
WHEN SLE.[Posted Invoice No_]&lt;&gt;'' THEN SLE.[Posted Invoice No_]
WHEN SLE.[Create Invoice No_]&lt;&gt;'' AND SH.[Current Stage]='ПЕРЕДАН' THEN SH.[Posting No_]
ELSE '' END [№ счета]
FROM
(SELECT [GTDNo_], [Posting Date], [Customer No_], [Responsibility Center], [Service Ledger Entry No_]
 FROM dbo.[NAVGTD Ledger Entry]
 WHERE [Journal Template Name]='ГТД' AND [Responsibility Center] ='КРЁКШИНО2' AND [Posting Date]  between @dStart and @dEnd 
 GROUP BY [GTDNo_], [Posting Date], [Customer No_], [Responsibility Center], [Service Ledger Entry No_]
) GLE

JOIN dbo.[NAVServices Ledger Entry] SLE ON 
GLE.[Service Ledger Entry No_]=SLE.[Entry No_]

LEFT JOIN dbo.[NAVSales Header] SH ON
SLE.[Create Invoice No_]=SH.[No_]

LEFT JOIN dbo.[NAVCustomer] Cust
ON GLE.[Customer No_]=Cust.[No_]

UNION

SELECT 
GJL.[GTDNo_], 
GJL.[GTDDate], 
CASE WHEN Cust.[Name] IS NOT NULL THEN Cust.[Name] + Cust.[Name 2] ELSE '' END [Импортер/Экспортер],
GJL.[Responsibility Center],
NULL,
''
FROM dbo.[NAVGTD Journal Line] GJL

LEFT JOIN dbo.[NAVCustomer] Cust
ON GJL.[Customer No_]=Cust.[No_]

WHERE GJL.[Journal Template Name]='ГТД' AND GJL.[Journal Batch Name]='СТАНДАРТ' AND GJL.[Responsibility Center] ='КРЁКШИНО2' AND GJL.[GTDDate]  between @dStart and @dEnd
GROUP BY GJL.[GTDNo_], GJL.[GTDDate], CASE WHEN Cust.[Name] IS NOT NULL THEN Cust.[Name] + Cust.[Name 2] ELSE '' END, GJL.[Responsibility Center]

ORDER BY GLE.[Responsibility Center], GLE.[Posting Date], GLE.[GTDNo_]</CommandText>
