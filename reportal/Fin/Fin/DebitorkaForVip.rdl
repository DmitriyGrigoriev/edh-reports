      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     tAllDocs.[1C Invoice Type], tAllDocs.[Global Dimension 2 Code] AS Grp, tAllDocs.[Global Dimension 1 Code] AS CFO, tAllDocs.[Posting Date], 
                      tAllDocs.[Customer No_], tAllDocs.[Currency Code], tAllDocs.DebDate, tAllDocs.SAStart, tAllDocs.SAEnd, NAVCustomer.Name AS FName, 
                      Cust2Man.ManName, NAVCustomer.[VAT Registration No_] AS INN, aCurs.CursVal, aCurs.CursRub, 
                      CASE WHEN tAllDocs.[Currency Code] &gt; '' THEN SAStart * aCurs.CursVal / aCurs.CursRub ELSE SAStart END AS sAmountStart, 
                      CASE WHEN tAllDocs.[Currency Code] &gt; '' THEN SAEnd * aCurs.CursVal / aCurs.CursRub ELSE SAEnd END AS sAmountEnd, 
                      CASE WHEN tAllDocs.[Currency Code] &gt; '' THEN SAStart ELSE NULL END AS OrAmountStart, 
                      CASE WHEN tAllDocs.[Currency Code] &gt; '' THEN SAEnd ELSE NULL END AS OrAmountEnd, tAllDocs.[Document No_], ISNULL
                          ((SELECT     TOP (1) Comment
                              FROM         aNavCust_Ledger_Comment
                              WHERE     ([Document No_] = tAllDocs.[Document No_])
                              ORDER BY IdComment DESC), 'Комментарий отсутствует') AS Comment
FROM         (SELECT     [NAVCust_ Ledger Entry].[1C Invoice Type], [NAVCust_ Ledger Entry].[Global Dimension 2 Code], [NAVCust_ Ledger Entry].[Document No_], 
                                              [NAVCust_ Ledger Entry].[Global Dimension 1 Code], [NAVCust_ Ledger Entry].[Posting Date], [NAVCust_ Ledger Entry].[Customer No_], 
                                              [NAVCust_ Ledger Entry].[Currency Code], @DateStart AS DebDate, SUM([NAVDetailed Cust_ Ledg_ Entry].Amount) AS SAStart, 
                                              0 AS SAEnd
                       FROM          [NAVDetailed Cust_ Ledg_ Entry] INNER JOIN
                                              [NAVCust_ Ledger Entry] ON [NAVDetailed Cust_ Ledg_ Entry].[Cust_ Ledger Entry No_] = [NAVCust_ Ledger Entry].[Entry No_]
                       WHERE      ([NAVCust_ Ledger Entry].[Document Type] = 2) AND ([NAVDetailed Cust_ Ledg_ Entry].[Posting Date] &lt;= @DateStart) AND 
                                              ([NAVCust_ Ledger Entry].[Global Dimension 2 Code] IN (@GRP)) AND ([NAVCust_ Ledger Entry].[1C Invoice Type] IN (@Uchet)) AND 
                                              ([NAVCust_ Ledger Entry].[Posting Date] &gt; '2010-01-01') AND ([NAVCust_ Ledger Entry].[Global Dimension 1 Code] IN (@CFO))
                       GROUP BY [NAVCust_ Ledger Entry].[1C Invoice Type], [NAVCust_ Ledger Entry].[Global Dimension 2 Code], 
                                              [NAVCust_ Ledger Entry].[Global Dimension 1 Code], [NAVCust_ Ledger Entry].[Posting Date], [NAVCust_ Ledger Entry].[Document No_], 
                                              [NAVCust_ Ledger Entry].[Customer No_], [NAVCust_ Ledger Entry].[Currency Code]
                       HAVING      (SUM([NAVDetailed Cust_ Ledg_ Entry].Amount) &lt;&gt; 0)
                       UNION
                       SELECT     [NAVCust_ Ledger Entry_1].[1C Invoice Type], [NAVCust_ Ledger Entry_1].[Global Dimension 2 Code], 
                                             [NAVCust_ Ledger Entry_1].[Document No_], [NAVCust_ Ledger Entry_1].[Global Dimension 1 Code], 
                                             [NAVCust_ Ledger Entry_1].[Posting Date], [NAVCust_ Ledger Entry_1].[Customer No_], [NAVCust_ Ledger Entry_1].[Currency Code], 
                                             @DateEnd AS DebDate, 0 AS SAStart, SUM([NAVDetailed Cust_ Ledg_ Entry_1].Amount) AS SAEnd
                       FROM         [NAVDetailed Cust_ Ledg_ Entry] AS [NAVDetailed Cust_ Ledg_ Entry_1] INNER JOIN
                                             [NAVCust_ Ledger Entry] AS [NAVCust_ Ledger Entry_1] ON 
                                             [NAVDetailed Cust_ Ledg_ Entry_1].[Cust_ Ledger Entry No_] = [NAVCust_ Ledger Entry_1].[Entry No_]
                       WHERE     ([NAVCust_ Ledger Entry_1].[Document Type] = 2) AND ([NAVDetailed Cust_ Ledg_ Entry_1].[Posting Date] &lt;= @DateEnd) AND 
                                             ([NAVCust_ Ledger Entry_1].[Global Dimension 2 Code] IN (@GRP)) AND ([NAVCust_ Ledger Entry_1].[1C Invoice Type] IN (@Uchet)) AND 
                                             ([NAVCust_ Ledger Entry_1].[Posting Date] &gt; '2010-01-01') AND ([NAVCust_ Ledger Entry_1].[Global Dimension 1 Code] IN (@CFO))
                       GROUP BY [NAVCust_ Ledger Entry_1].[1C Invoice Type], [NAVCust_ Ledger Entry_1].[Global Dimension 2 Code], 
                                             [NAVCust_ Ledger Entry_1].[Global Dimension 1 Code], [NAVCust_ Ledger Entry_1].[Posting Date], 
                                             [NAVCust_ Ledger Entry_1].[Document No_], [NAVCust_ Ledger Entry_1].[Customer No_], [NAVCust_ Ledger Entry_1].[Currency Code]
                       HAVING      (SUM([NAVDetailed Cust_ Ledg_ Entry_1].Amount) &lt;&gt; 0)) AS tAllDocs LEFT OUTER JOIN
                      aCurs ON tAllDocs.[Posting Date] &lt; aCurs.EndDate AND tAllDocs.[Posting Date] &gt;= aCurs.StartDate AND 
                      tAllDocs.[Currency Code] = aCurs.VidVal LEFT OUTER JOIN
                      NAVCustomer ON tAllDocs.[Customer No_] = NAVCustomer.No_ LEFT OUTER JOIN
                          (SELECT     dbo.ConcatDistinct(aLogins.Name) AS ManName, aACL.aValue AS CustNo_
                            FROM          aACL INNER JOIN
                                                   aLogins ON aACL.aLogin = aLogins.Login
                            WHERE      (aACL.aType = 'CUSTNo2MNG') AND (aACL.aLogin &lt;&gt; 'NTBROKER\kolyan') AND (aACL.aLogin &lt;&gt; 'NTBROKER\alutay') AND 
                                                   (aACL.aLogin &lt;&gt; 'NTBROKER\ubondarenko')
                            GROUP BY aACL.aValue) AS Cust2Man ON NAVCustomer.No_ = Cust2Man.CustNo_</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DateStart">
            <Value>=Parameters!DateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GRP">
            <Value>=Parameters!GRP.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Uchet">
            <Value>=Parameters!Uchet.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CFO">
            <Value>=Parameters!CFO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateEnd">
            <Value>=Parameters!DateEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="GRP">
      <Fields>
        <Field Name="Global_Dimension_2_Code">
          <DataField>Global Dimension 2 Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     [Global Dimension 2 Code]
FROM         [NAVCust_ Ledger Entry]
WHERE     ([Global Dimension 2 Code] &gt; '')
GROUP BY [Global Dimension 2 Code]
ORDER BY [Global Dimension 2 Code]</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="GRPdef">
      <Fields>
        <Field Name="Global_Dimension_2_Code">
          <DataField>Global Dimension 2 Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     [Global Dimension 2 Code]
FROM         [NAVCust_ Ledger Entry]
GROUP BY [Global Dimension 2 Code]
ORDER BY [Global Dimension 2 Code]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
