  <DataSets>
    <DataSet Name="LocalFTS">
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DocNo">
            <Value>=Parameters!DocNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RepName">
            <Value>=Parameters!RepName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>INSERT INTO aLog
                      (aDate, aLogin, aReport)
VALUES     (GetDate(), SYSTEM_USER,@RepName); 
SELECT     Comment, Author, Date, [Document No_], IdComment
FROM         aNavCust_Ledger_Comment
WHERE     ([Document No_] = @DocNo)
ORDER BY IdComment</CommandText>
      </Query>
