  <DataSets>
    <DataSet Name="LocalFTS">
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SD">
            <Value>=Parameters!SD.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RepName">
            <Value>=Parameters!RepName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ED">
            <Value>=Parameters!ED.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CFO">
            <Value>=Parameters!CFO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MBE">
            <Value>=Parameters!MBE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Bjt">
            <Value>=Parameters!Bjt.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>INSERT INTO aLog
                      (aDate, aLogin, aReport)
VALUES     (GetDate(), SYSTEM_USER,@RepName); 
SELECT     aPL.ID, aPL.Name, SUM(t .P * aPL.K) AS sP, SUM(t .F * aPL.K) AS sF, aPL.C, aPL.M, aPL.MName
FROM         (SELECT     CONVERT(Money, SUM(Amount)) AS F, CONVERT(Money, 0) AS P, Code, Month([Posting Date]) AS Mon
                       FROM          Cube_Rashod_Budjet_Fact
                       WHERE      ([Posting Date] BETWEEN @SD AND @ED) AND (CFO IN (@CFO)) and (IsNull(MBE,'Null') like @MBE)
                       GROUP BY Code, Month([Posting Date])
                       UNION
                       SELECT     CONVERT(Money, 0) AS F, CONVERT(Money, SUM(Amount)) AS P, Code, Month(Date) AS Mon
                       FROM         Cube_Rashod_Budjet_Plan
                       WHERE     (Date BETWEEN @SD AND @ED) AND (CFO IN (@CFO)) AND ([Budget Name] IN (@Bjt))
                       GROUP BY Code, Month(Date)) AS t RIGHT OUTER JOIN
                          (SELECT     aPL.ID, aPL.Code, aPL.Name, aPL.K, aPL.C, m.MName, m.ID AS M
                            FROM          aPL CROSS JOIN
                                                       (SELECT     MName, ID
                                                         FROM          aMonthQurtal) AS m) AS aPL ON t.Code LIKE aPL.Code AND M = Mon
GROUP BY aPL.Name, aPL.ID, aPL.C, aPL.M, aPL.MName
Union
SELECT     aPL.ID, aPL.Name, SUM(t .P * aPL.K) AS sP, SUM(t .F * aPL.K) AS sF, aPL.C, 0 As M, aPL.MName
FROM         (SELECT     CONVERT(Money, SUM(Amount)) AS F, CONVERT(Money, 0) AS P, Code
                       FROM          Cube_Rashod_Budjet_Fact
                       WHERE      ([Posting Date] BETWEEN @SD AND @ED) AND (CFO IN (@CFO)) and (IsNull(MBE,'Null') like @MBE)
                       GROUP BY Code
                       UNION
                       SELECT     CONVERT(Money, 0) AS F, CONVERT(Money, SUM(Amount)) AS P, Code
                       FROM         Cube_Rashod_Budjet_Plan
                       WHERE     (Date BETWEEN @SD AND @ED) AND (CFO IN (@CFO)) AND ([Budget Name] IN (@Bjt))
                       GROUP BY Code) AS t RIGHT OUTER JOIN
                          (SELECT     aPL.ID, aPL.Code, aPL.Name, aPL.K, aPL.C, 'ИТОГО' As MName
                            FROM          aPL ) AS aPL ON t.Code LIKE aPL.Code
GROUP BY aPL.Name, aPL.ID, aPL.C, aPL.MName
ORDER BY ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
