    <DataSet Name="CFO">
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     Cube_FTS_OUR.[Global Dimension 1 Code] AS CFO, 'ЦФО ' + Cube_FTS_OUR.[Global Dimension 1 Code] AS Val
FROM         Cube_FTS_OUR INNER JOIN
                          (SELECT     aValue
                            FROM          aACL
                            WHERE      (aType = 'CFO') AND (aLogin IN
                                                       (SELECT     SYSTEM_USER AS tLogin))) AS tCFO ON Cube_FTS_OUR.[Global Dimension 1 Code] LIKE tCFO.aValue
GROUP BY Cube_FTS_OUR.[Global Dimension 1 Code]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CFO">
          <DataField>CFO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Val">
          <DataField>Val</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Bjt">
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     [Budget Name]
FROM         Cube_Rashod_Budjet_Plan
GROUP BY [Budget Name]
ORDER BY [Budget Name]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
