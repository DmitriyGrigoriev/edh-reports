  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SD">
            <Value>=Parameters!SD.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RepName">
            <Value>=Parameters!RepName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ED">
            <Value>=Parameters!ED.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>INSERT INTO aLog (aDate, aLogin, aReport)
VALUES (GetDate(), SYSTEM_USER,@RepName); 
SELECT SH.[Document No_], SH.[Posting Date],
       SH.[Customer No_], C.[Name] + C.[Name 2] AS [C_Name],
       ISNULL(SL1.[Ставка Перевозчика (ФАКТ), руб],0) AS [St],
       ISNULL(SL2.[Услуги для выставления (ФАКТ), руб],0) AS [Usl],
       ISNULL(SL2.[Услуги для выставления (ФАКТ), руб],0) - ISNULL(SL1.[Ставка Перевозчика (ФАКТ), руб],0) AS [Doh],
       ISNULL(SL3.[SalIH Posting Date],SL3.[SalH Posting Date]) AS [SalH Posting Date]
FROM [olap].[LocalFts].[dbo].[NAVShipment Order Journal Line] AS SH LEFT OUTER JOIN
     (SELECT [Shipment Order Line No_], [Journal Template Name], ROUND(sum([Amount (LCY)]),2) AS [Ставка Перевозчика (ФАКТ), руб]
      FROM [olap].[LocalFts].[dbo].[NAVSO Service Journal Line]
      WHERE [Line Type]=2
      GROUP BY [Shipment Order Line No_], [Journal Template Name]) SL1
         ON SH.[Line No_]=SL1.[Shipment Order Line No_] AND SH.[Journal Template Name]=SL1.[Journal Template Name]
         LEFT OUTER JOIN
     (SELECT [Shipment Order Line No_], [Journal Template Name], ROUND(sum([Amount (LCY)]),2) AS [Услуги для выставления (ФАКТ), руб]
      FROM [olap].[LocalFts].[dbo].[NAVSO Service Journal Line]
      WHERE [Line Type]=1
      GROUP BY [Shipment Order Line No_], [Journal Template Name]) SL2
         ON SH.[Line No_]=SL2.[Shipment Order Line No_] AND SH.[Journal Template Name]=SL2.[Journal Template Name]
		 LEFT OUTER JOIN
     (SELECT SJL.[Shipment Order Line No_], SJL.[Journal Template Name], max(SalH.[Posting Date]) AS [SalH Posting Date], max(SalIH.[Posting Date]) AS [SalIH Posting Date]
      FROM [olap].[LocalFts].[dbo].[NAVSO Service Journal Line] AS SJL
              INNER JOIN
           [olap].[LocalFts].[dbo].[NAVServices Ledger Entry] AS SLE
              ON SJL.[Service Ledger Entry No_]=SLE.[Entry No_]
              LEFT OUTER JOIN
           [olap].[LocalFts].[dbo].[NAVSales Header] AS SalH
              ON SLE.[Create Invoice No_]=SalH.[No_]
              LEFT OUTER JOIN
           [olap].[LocalFts].[dbo].[NAVSales Invoice Header] AS SalIH
              ON SLE.[Posted Invoice No_]=SalIH.[No_]
      WHERE SJL.[Line Type]=1
      GROUP BY SJL.[Shipment Order Line No_], SJL.[Journal Template Name]) SL3
         ON SH.[Line No_]=SL3.[Shipment Order Line No_] AND SH.[Journal Template Name]=SL3.[Journal Template Name]
         LEFT OUTER JOIN
     [olap].[LocalFts].[dbo].[NAVCustomer] AS C
         ON SH.[Customer No_]=C.[No_]
Where SH.[Posting Date]&gt;=@SD
  and SH.[Posting Date]&lt;=@ED
UNION ALL
SELECT SH.[Document No_], SH.[Posting Date],
       SH.[Customer No_] AS [Клиент Код], C.[Name] + C.[Name 2] AS [C_Name],
       ISNULL(SL1.[Ставка Перевозчика (ФАКТ), руб],0) AS [St],
       ISNULL(SL2.[Услуги для выставления (ФАКТ), руб],0) AS [Usl],
       ISNULL(SL2.[Услуги для выставления (ФАКТ), руб],0) - ISNULL(SL1.[Ставка Перевозчика (ФАКТ), руб],0) AS [Doh],
       ISNULL(SL3.[SalIH Posting Date],SL3.[SalH Posting Date]) AS [SalH Posting Date]
FROM [olap].[LocalFts].[dbo].[NAVShipment Order Ledger Entry] AS SH
         LEFT OUTER JOIN
     (SELECT [Shipment Order Entry No_], [Journal Template Name], ROUND(sum([Amount (LCY)]),2) AS [Ставка Перевозчика (ФАКТ), руб]
      FROM [olap].[LocalFts].[dbo].[NAVSO Service Ledger Entry]
      WHERE [Line Type]=2
      GROUP BY [Shipment Order Entry No_], [Journal Template Name]) SL1
         ON SH.[Entry No_]=SL1.[Shipment Order Entry No_] AND SH.[Journal Template Name]=SL1.[Journal Template Name]
		 LEFT OUTER JOIN
     (SELECT [Shipment Order Entry No_], [Journal Template Name], ROUND(sum([Amount (LCY)]),2) AS [Услуги для выставления (ФАКТ), руб]
      FROM [olap].[LocalFts].[dbo].[NAVSO Service Ledger Entry]
      WHERE [Line Type]=1
      GROUP BY [Shipment Order Entry No_], [Journal Template Name]) SL2
         ON SH.[Entry No_]=SL2.[Shipment Order Entry No_] AND SH.[Journal Template Name]=SL2.[Journal Template Name]
		 LEFT OUTER JOIN
     (SELECT SJL.[Shipment Order Entry No_], SJL.[Journal Template Name], max(SalH.[Posting Date]) AS [SalH Posting Date], max(SalIH.[Posting Date]) AS [SalIH Posting Date]
      FROM [olap].[LocalFts].[dbo].[NAVSO Service Ledger Entry] AS SJL
               INNER JOIN
           [olap].[LocalFts].[dbo].[NAVServices Ledger Entry] AS SLE
               ON SJL.[Service Ledger Entry No_]=SLE.[Entry No_]
               LEFT OUTER JOIN
           [olap].[LocalFts].[dbo].[NAVSales Header] AS SalH
               ON SLE.[Create Invoice No_]=SalH.[No_]
               LEFT OUTER JOIN
           [olap].[LocalFts].[dbo].[NAVSales Invoice Header] AS SalIH
               ON SLE.[Posted Invoice No_]=SalIH.[No_]
      WHERE SJL.[Line Type]=1
      GROUP BY SJL.[Shipment Order Entry No_], SJL.[Journal Template Name]) SL3
         ON SH.[Entry No_]=SL3.[Shipment Order Entry No_] AND SH.[Journal Template Name]=SL3.[Journal Template Name]
         LEFT OUTER JOIN
     [olap].[LocalFts].[dbo].[NAVCustomer] AS C
         ON SH.[Customer No_]=C.[No_]
Where SH.[Posting Date]&gt;=@SD
  and SH.[Posting Date]&lt;=@ED
ORDER BY 1</CommandText>
