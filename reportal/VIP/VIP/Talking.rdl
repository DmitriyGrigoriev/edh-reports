  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ED">
            <Value>=Parameters!ED.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SD">
            <Value>=Parameters!SD.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select fio, isnull(number,[Кто звонил]) as n, sum(DatePart(hh, [Длительность]))*60+sum(DatePart(n, [Длительность])) as aSec, [Куда звонил] as W
From olap.From1s.dbo.AsterFIO full join
    olap.From1s.dbo.AsterLog on number=[Кто звонил]
where [Дата звонка]&lt;=@ED
  and [Дата звонка]&gt;=@SD
Group by fio, isnull(number,[Кто звонил]), [Куда звонил]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
