  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>DataSourceReportal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndtDate">
            <Value>=Parameters!EndtDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Cfo">
            <Value>=Parameters!Cfo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select isnull(dtFact.inn,tFact.inn) as inn, sFact, Dt,
       (Select FName
	    From [OLAP].[LocalFTS].[dbo].Cube_Firms
		where inn=isnull(dtFact.inn,tFact.inn)) as Fname
From (Select Sum(AmountRub) as sFact, inn
	  From [OLAP].[LocalFTS].[dbo].Cube_Money
	  Where [GD1]&gt;=@StartDate
        and [GD1]&lt;=@EndtDate
        and (Select top 1 [Global Dimension 1 Code]
		     From [OLAP].[LocalFTS].[dbo].Cube_FTS_OUR
			 Where G071=Cube_Money.G071
			   and BeginDate&lt;=Cube_Money.GD1
			 order by BeginDate desc) in (@Cfo)
	  Group by inn) as tFact full join
     (Select Sum(Gtd) as Dt, Inn
	  From [OLAP].[LocalFTS].[dbo].Cube_Decl
	  Where [GD1]&gt;=@StartDate
        and [GD1]&lt;=@EndtDate
		and (Select top 1 [Global Dimension 1 Code]
		     From [OLAP].[LocalFTS].[dbo].Cube_FTS_OUR
		     Where G071=Cube_Decl.G071
			   and BeginDate&lt;=Cube_Decl.GD1
			 order by BeginDate desc) in (@Cfo)
	  Group by inn) as dtFact on dtFact.inn=tFact.inn</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="inn">
          <DataField>inn</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sFact">
          <DataField>sFact</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Dt">
          <DataField>Dt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Fname">
          <DataField>Fname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="cfo">
      <Query>
        <DataSourceName>DataSourceReportal</DataSourceName>
        <CommandText>SELECT     [Global Dimension 1 Code] AS CFO
FROM         Cube_FTS_OUR
GROUP BY [Global Dimension 1 Code]
ORDER BY CFO</CommandText>
      </Query>
      <Fields>
        <Field Name="CFO">
          <DataField>CFO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="cfolist">
      <Query>
        <DataSourceName>DataSourceReportal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@cfolist">
            <Value>=Parameters!cfolist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     [Global Dimension 1 Code] AS CFO
FROM         Cube_FTS_OUR
GROUP BY [Global Dimension 1 Code]
having @cfolist like '%*'+[Global Dimension 1 Code]+'*%'
   or @cfolist is null
ORDER BY CFO</CommandText>
      </Query>
      <Fields>
        <Field Name="CFO">
          <DataField>CFO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
