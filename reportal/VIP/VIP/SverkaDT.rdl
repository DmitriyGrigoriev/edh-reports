  <DataSets>
    <DataSet Name="LocalFTS">
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CFO">
            <Value>=Parameters!CFO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RepName">
            <Value>=Parameters!RepName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ED">
            <Value>=Parameters!ED.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SD">
            <Value>=Parameters!SD.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>INSERT INTO aLog (aDate, aLogin, aReport)
VALUES     (GetDate(), SYSTEM_USER,@RepName); 
Select ROW_NUMBER() OVER(ORDER by GD1, G07X) AS RowID, GD1, G07X, G092, G022, G082, G011
From tDclHead
Where G071 in (Select G071 From Cube_FTS_OUR Where  [Global Dimension 1 Code] in (@CFO))
  and GD1&lt;=@ED
  and GD1&gt;=@SD</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
