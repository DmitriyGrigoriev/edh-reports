        <CommandText>INSERT INTO aLog (aDate, aLogin, aReport)
VALUES (GetDate(), SYSTEM_USER,@RepName); 
Select inn, sum(sUsl) as fUsl, sum(sMge) as fMge, sum(sSert) as fSert, sum(Dt) as fDt,
       (Select FName
	    From [OLAP].[LocalFTS].[dbo].Cube_Firms
		where inn=forDaysFact.inn) as Fname,
	   (Select top 1 convert(VarChar(20),spDT, 104) From [OLAP].[LocalFTS].[dbo].forDaysAll) as spDT
From olap.LocalFts.dbo.forDaysFact
Where Year(wGD1)=@Year
  and Month(wGD1)=@Month
  and CFO in (@CFO)
Group By inn;</CommandText>
