      <Query>
        <DataSourceName>arm_smolensk</DataSourceName>
        <CommandText>SELECT     DECL.ND AS [№ ГТД], TOVG.G31_15 AS Артикул, TOV.G33 AS [Код ТНВЭД], TOV.G34 AS [Страна происхождения (код)], 
                      TOV.G32 AS [№ товара в ГТД], DECL.G081, DECL.G082
FROM         DBRTOVG AS TOVG RIGHT OUTER JOIN
                      DCLTOVAR AS TOV ON TOVG.G32 = TOV.G32 AND TOVG.DocNum = TOV.ND RIGHT OUTER JOIN
                      DCLHEAD AS DECL ON TOV.ND = DECL.ND
WHERE     (DECL.G081 = '7728750964')</CommandText>
      </Query>
