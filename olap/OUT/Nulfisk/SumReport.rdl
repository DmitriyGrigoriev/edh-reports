        <CommandText>Select *
From [dbo].[Cube_Firms] f
where exists(select p.g07X
from tdclSumPP p left join
	tdclHead d on p.g07X=d.g07X left join
	tdclhead h on p.g07X=h.g07X
where (h.g021 =f.inn or h.g081 =f.inn or h.g091=f.inn)
