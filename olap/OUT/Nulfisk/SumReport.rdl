  <DataSets>
    <DataSet Name="main">
      <Query>
        <DataSourceName>main</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@sd">
            <Value>=Parameters!sd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inn">
            <Value>=Parameters!inn.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ed">
            <Value>=Parameters!ed.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Cfo">
            <Value>=Parameters!Cfo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select p.g07X,d.gd1 as gtdOut,d.g011+' '+d.g0121+' '+d.g0131 as inf,
	row_number() over(PARTITION by p.g07x order by p.nzp) as n,
	p.nzp,p.gb1 as vidSpis,
	p.innPlat, p.kppPlat, p.numPDok, p.sumPDok, p.sum_all,
	p.tModify, p.dModify, p.datPDok 
from tdclSumPP p left join
	tdclHead d on p.g07X=d.g07X left join
	CUBE_DECL c on p.g07X=c.g07X left join
	Cube_FTS_OUR cfo ON c.oID = cfo.id
where (d.g021 =@inn or d.g081 =@inn or d.g091=@inn)
   and d.gd1&gt;=@sd and d.gd1&lt;=@ed
