        </Field>
        <Field Name="Подразделение">
          <DataField>Подразделение</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="P">
          <DataField>P</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>tmpPropusk</DataSourceName>
        <CommandText>Select *, Convert(VarChar(10), Время, 104) + ' ' + Convert(VarChar(8), Время, 14) as dt
From (Select (Select Count(*) As C
        From source_Крёкшино As tLow
        Where Convert(VarChar(10), tTop.Время, 104)=Convert(VarChar(10), tLow.Время, 104)
          and tTop.Время&gt;tLow.Время and tLow.Фамилия=tTop.Фамилия) as n,
       *,
       (Select Case When Convert(VarChar(8), Max(tLow2.Время), 14)&lt;'17:50' Then 'Рано'
                    When Convert(VarChar(8), Min(tLow2.Время), 14)&gt;'09:10' Then 'Поздно'
                    Else 'Ok' End as S
        From source_Крёкшино As tLow2
        Where Convert(VarChar(10), tTop.Время, 104)=Convert(VarChar(10), tLow2.Время, 104)
          and tLow2.Фамилия=tTop.Фамилия) as P
From source_Крёкшино As tTop) as tAll
Where [Время]&gt;= @DateBegin and [Время]&lt;= @DateEnd and [Фамилия] like @fio and P like @P
Order By Фамилия, Время</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DateBegin">
            <Value>=Parameters!DateBegin.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateEnd">
            <Value>=Parameters!DateEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@fio">
            <Value>=Parameters!fio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@P">
            <Value>=Parameters!P.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="fio">
      <Fields>
        <Field Name="l">
          <DataField>l</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="v">
          <DataField>v</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>tmpPropusk</DataSourceName>
        <CommandText>Select Фамилия as l, Фамилия as v
From source_Крёкшино
Group By Фамилия
Union
Select ' Все' as l, '%' as v
Order By l</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
