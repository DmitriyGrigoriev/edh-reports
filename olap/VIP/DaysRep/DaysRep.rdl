      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     ID, GD1, CFO, pGTD, sGTD, nGTD, dGTD, pSum, sSum, nSum, dSum, Days, LDay
FROM         dbo.repDays(@Y + '-' + @M + '-' + @M)
WHERE     (CFO IN (@CFO))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Y">
            <Value>=Parameters!Y.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@M">
            <Value>=Parameters!M.Value</Value>
          </QueryParameter>
