      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     [Global Dimension 1 Code] AS CFO
FROM         Cube_FTS_OUR
GROUP BY [Global Dimension 1 Code]
ORDER BY CFO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
