        <Field Name="pGTD">
          <DataField>pGTD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="sGTD">
          <DataField>sGTD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="pSum">
          <DataField>pSum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="sSum">
          <DataField>sSum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>SELECT     MIN(GD1) AS GD1, CFO, SUM(pGTD) AS pGTD, SUM(sGTD) AS sGTD, SUM(pSum) AS pSum, SUM(sSum) AS sSum
FROM         (SELECT     CUBE_Money.GD1, Cube_FTS_OUR_2.[Global Dimension 1 Code] AS CFO, CONVERT(int, NULL) AS pGTD, CONVERT(int, NULL) AS sGTD, 
                                              CONVERT(Money, NULL) AS pSum, SUM(CUBE_Money.AmountRub) AS sSum
                       FROM          CUBE_Money INNER JOIN
                                              Cube_FTS_OUR AS Cube_FTS_OUR_2 ON CUBE_Money.G071 = Cube_FTS_OUR_2.G071
                       WHERE      (YEAR(CUBE_Money.GD1) =@Y) And [Global Dimension 1 Code]In (@CFO)
                       GROUP BY CUBE_Money.GD1, Cube_FTS_OUR_2.[Global Dimension 1 Code]
                       UNION
                       SELECT     Cube_PlanGTD.GD1, Cube_FTS_OUR_1.[Global Dimension 1 Code] AS CFO, SUM(Cube_PlanGTD.PlanGTD) AS pGTD, CONVERT(int, NULL) 
                                             AS sGTD, SUM(Cube_PlanGTD.PlanVyruchki) AS pSum, CONVERT(Money, NULL) AS sSum
                       FROM         Cube_PlanGTD INNER JOIN
                                             Cube_FTS_OUR AS Cube_FTS_OUR_1 ON Cube_PlanGTD.G071 = Cube_FTS_OUR_1.G071
                       WHERE     (YEAR(Cube_PlanGTD.GD1) = @Y) And [Global Dimension 1 Code]In (@CFO)
                       GROUP BY Cube_PlanGTD.GD1, Cube_FTS_OUR_1.[Global Dimension 1 Code]
                       UNION
                       SELECT     CUBE_DECL.GD1, Cube_FTS_OUR.[Global Dimension 1 Code] AS CFO, CONVERT(int, NULL) AS pGTD, SUM(CUBE_DECL.GTD) AS sGTD, 
                                             CONVERT(Money, NULL) AS pSum, CONVERT(Money, NULL) AS sSum
                       FROM         CUBE_DECL INNER JOIN
                                             Cube_FTS_OUR ON CUBE_DECL.G071 = Cube_FTS_OUR.G071
                       WHERE     (YEAR(CUBE_DECL.GD1) = @Y) And [Global Dimension 1 Code]In (@CFO)
                       GROUP BY CUBE_DECL.GD1, Cube_FTS_OUR.[Global Dimension 1 Code]) AS tFull
GROUP BY MONTH(GD1), CFO
ORDER BY CFO, GD1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Y">
            <Value>=Parameters!Y.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CFO">
            <Value>=Parameters!CFO.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
