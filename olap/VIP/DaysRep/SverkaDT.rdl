      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>Select ROW_NUMBER() OVER(ORDER by GD1, G07X) AS RowID, GD1, G07X, G092, G022, G082, G011
From tDclHead
Where G071 in (Select G071 From Cube_FTS_OUR Where  [Global Dimension 1 Code] in (@CFO))
  and GD1&lt;=@ED
  and GD1&gt;=@SD</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CFO">
            <Value>=Parameters!CFO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ED">
            <Value>=Parameters!ED.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SD">
            <Value>=Parameters!SD.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="CFO">
      <Fields>
        <Field Name="Global_Dimension_1_Code">
          <DataField>Global Dimension 1 Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>Select DISTINCT  [Global Dimension 1 Code] 
From Cube_FTS_OUR join
     aACL on  [Global Dimension 1 Code] like aValue and  (aLogin = SYSTEM_USER) AND (aType = 'CFOSverka')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
