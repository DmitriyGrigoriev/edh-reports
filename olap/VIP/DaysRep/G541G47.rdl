    <DataSet Name="LocalFTS">
      <Fields>
        <Field Name="M">
          <DataField>M</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DT">
          <DataField>DT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="sG472_1">
          <DataField>sG472_1</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="sG474_1">
          <DataField>sG474_1</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="sG474_2">
          <DataField>sG474_2</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="sG474_5">
          <DataField>sG474_5</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>Select month(gd1) as M, count(*) as DT, sum(G472_1) as sG472_1
	, sum(G474_1) as sG474_1, sum(G474_2) as sG474_2, sum(G474_5) as sG474_5
From tDclHead left join
	(Select max(G472) as G472_1, G07x
	From tDclPLATR
	where g471 like '1%'
	group by G07x) as G472_1 on tDclHead.G07X=G472_1.G07X left join
	(Select max(G474) as G474_1, G07x
	From tDclPLATR
	where g471 like '1%'
	group by G07x) as G474_1 on tDclHead.G07X=G474_1.G07X left join
	(Select max(G474) as G474_2, G07x
	From tDclPLATR
	where g471 like '2%'
	group by G07x) as G474_2 on tDclHead.G07X=G474_2.G07X left join
	(Select max(G474) as G474_5, G07x
	From tDclPLATR
	where g471 like '5%'
	group by G07x) as G474_5 on tDclHead.G07X=G474_5.G07X
where year(GD1)=@year
	and G541 like '0066/%'
	and left(GD0,1)&lt;'8'
Group By month(gd1)
order by m;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="yers">
      <Fields>
        <Field Name="y">
          <DataField>y</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LocalFTS</DataSourceName>
        <CommandText>Select year(GD1) as y
From tDclHead
where not gd1 is null
Group by year(GD1)
Order by y</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
